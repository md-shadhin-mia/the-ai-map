{
  "manifest_version": 3,
  "name": "API Integration Extension",
  "version": "1.0",
  "description": "A simple extension to interact with a local API.",
  "permissions": [
    "activeTab",
    "scripting",
    "notifications"
  ],
  "host_permissions": [
    "http://localhost:3000/*"
  ],
  "action": {
    "default_popup": "popup.html"
  
  },
  "background": {
    "service_worker": "background.js"
  }
}